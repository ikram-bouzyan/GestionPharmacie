@model List<Medicament>


<div class="container">
    <h1 class="mt-3">Les Midecaments de Pfizer</h1>

    <ul class="list-group mt-3">
        @foreach (Medicament med in ViewBag.MedicamentPfizer)
        {
            <li class="list-group-item d-flex justify-content-between align-items-center">
                <div>
                    <strong>Numero:</strong> @med.Numero |
                    <strong>Type:</strong> @med.NomFabriquant |
                    <strong>Etage:</strong> @med.DateExpiration |
                </div>
            </li>
        }
    </ul>


</div>
