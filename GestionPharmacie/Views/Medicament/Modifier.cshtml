@model GestionPharmacie.Models.Medicament

@{
    ViewData["Title"] = "Modifier Medicament";
}

<h1>Modifier Medicament</h1>

<form asp-controller="Medicament" asp-action="Modifier" method="post">
    <div class="form-group">
        <label asp-for="Numero "></label>
        <input asp-for="Numero" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="NomFabriquant"></label>
        <input asp-for="NomFabriquant" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="DateExpiration"></label>
        <input asp-for="DateExpiration" class="form-control" />
    </div>
    <br />
    <button type="submit" class="btn btn-primary">Modifier Medicament</button>

</form>

